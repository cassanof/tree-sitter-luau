{
  "name": "tree-sitter-luau",
  "version": "0.0.2",
  "description": "luau grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build --debug",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.luau",
      "file-types": [
        "luau"
      ],
      "first-line-regex": "--!(?:strict|nonstrict|nocheck)$"
    }
  ]
}
